<package>
  <description brief="pr2_arm_ik">
   This package provides a kinematics implementation for the PR2 robot. It can be used to compute forward and inverse kinematics. 
  </description>
  <author>Sachin Chitta</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/pr2_arm_ik</url>
  <depend package="roscpp"/>
  <depend package="kdl"/>
  <depend package="eigen"/>
  <depend package="angles"/>
  <depend package="tf_conversions"/>
  <depend package="urdf"/>
  <depend package="geometry_msgs"/>
  <depend package="kdl_parser"/>

  <depend package="kinematics_msgs" />

  <export>
   <cpp cflags="-I${prefix}/include" lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lpr2_arm_ik"/>
  </export>

</package>


